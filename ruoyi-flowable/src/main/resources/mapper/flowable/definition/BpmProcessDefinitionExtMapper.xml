        where id = #{id}
    </update>

    <delete id="deleteBpmProcessDefinitionExtById" parameterType="Long">
        delete from bpm_process_definition_ext where id = #{id}
    </delete>

    <delete id="deleteBpmProcessDefinitionExtByIds" parameterType="String">
        delete from bpm_process_definition_ext where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectListByProcessDefinitionIds" parameterType="String"
            resultMap="BpmProcessDefinitionExtResult">
        <include refid="selectBpmProcessDefinitionExtVo"/>
        <where>
