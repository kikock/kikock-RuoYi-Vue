    <sql id="selectBpmProcessDefinitionExtVo">
        select id,
               process_definition_id,
               model_id,
               description,
               form_type,
               form_id,
               form_conf,
               form_fields,
               form_custom_create_path,
               form_custom_view_path,
               del_flag,
               create_by,
               create_time,
               update_by,
               update_time,
               remark
        from bpm_process_definition_ext
