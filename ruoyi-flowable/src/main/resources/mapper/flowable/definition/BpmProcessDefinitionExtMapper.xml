        </where>
    </select>

    <select id="selectBpmProcessDefinitionExtById" parameterType="Long"
            resultMap="BpmProcessDefinitionExtResult">
        <include refid="selectBpmProcessDefinitionExtVo"/>
        where id = #{id}
    </select>

    <insert id="insertBpmProcessDefinitionExt" parameterType="BpmProcessDefinitionExt" useGeneratedKeys="true"
            keyProperty="id">
        insert into bpm_process_definition_ext
        <trim prefix="(" suffix=")" suffixOverrides=",">
