    <select id="selectBpmProcessDefinitionExtList" parameterType="BpmProcessDefinitionExt"
            resultMap="BpmProcessDefinitionExtResult">
        <include refid="selectBpmProcessDefinitionExtVo"/>
        <where>
            <if test="processDefinitionId != null  and processDefinitionId != ''">
                and process_definition_id =
                #{processDefinitionId}
            </if>
            <if test="modelId != null  and modelId != ''">
                and model_id =
                #{modelId}
            </if>
            <if test="description != null  and description != ''">
                and description =
                #{description}
            </if>
            <if test="formType != null">
                and form_type =
                #{formType}
            </if>
            <if test="formId != null">
                and form_id =
                #{formId}
            </if>
            <if test="formConf != null  and formConf != ''">
                and form_conf =
                #{formConf}
            </if>
            <if test="formFields != null  and formFields != ''">
                and form_fields =
                #{formFields}
            </if>
            <if test="formCustomCreatePath != null  and formCustomCreatePath != ''">
