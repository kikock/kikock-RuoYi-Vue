<mapper namespace="com.ruoyi.flowable.mapper.definition.BpmProcessDefinitionExtMapper">
    <resultMap type="BpmProcessDefinitionExt" id="BpmProcessDefinitionExtResult">
        <result property="id" column="id"/>
        <result property="processDefinitionId" column="process_definition_id"/>
        <result property="modelId" column="model_id"/>
        <result property="description" column="description"/>
        <result property="formType" column="form_type"/>
        <result property="formId" column="form_id"/>
        <result property="formConf" column="form_conf"/>
        <result property="formFields" column="form_fields"/>
        <result property="formCustomCreatePath" column="form_custom_create_path"/>
